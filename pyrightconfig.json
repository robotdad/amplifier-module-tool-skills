{
  "venvPath": ".",
  "venv": ".venv",
  "exclude": [
    "**/__pycache__",
    ".venv/**",
    "node_modules/**",
    ".git/**",
    ".ruff_cache/**",
    ".pytest_cache/**",
    "*.egg-info",
    "build/**",
    "dist/**"
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": false,
  "pythonVersion": "3.11"
}
